    (_, code) => `<code>${escapeHtml(code)}</code>`
  );

  // 3. Bold: **text** → <strong>text</strong>
  text = text.replace(
    /\*\*([^*\n]+?)\*\*/g,
    (_, bold) => `<strong>${bold}</strong>`
  );

  // 4. Italic: *text* → <em>text</em>
  text = text.replace(
    /\*([^*\n]+?)\*/g,
    (_, em) => `<em>${em}</em>`
  );

